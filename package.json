{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client --host lan",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "native:ios-dev": "expo run:ios",
    "native:ios-preview": "expo run:ios --device --configuration Release",
    "native:android": "expo run:android",
    "web": "expo start --web",
    "prebuild": "expo prebuild && xed ios && npm run dev",
    "type-check": "tsc --noEmit",
    "doctor": "npx expo-doctor",
    "lint": "expo lint",
    "generate-apk-file": "eas build --platform android --local --profile preview",
    "generate-ipa-file": "echo prebuild first then open ios xcworkspace and change the run scheme to release and build for local iphone then check Xcode-DerivedData-AppName-Build-Products-ReleaseIphoneOS** folder for released app file then convert wrap that into zip -r appname.ipa Payload file."
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.7",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-worklets": "0.5.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "expo-secure-store": "~13.0.2",
    "react-native-sse": "^1.1.2",
    "react-native-webview": "^13.8.4"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
